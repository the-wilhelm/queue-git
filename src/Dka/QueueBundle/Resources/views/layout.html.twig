<!DOCTYPE html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js"> <!--<![endif]-->
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>{% block title %}DKA Queue{% endblock %}</title>
    <meta name="description" content="{% block description %}DKA Queue{% endblock %}">
    <meta name="viewport" content="width=device-width">
	
    {% block stylesheets %}
		{% stylesheets
		 	'@DkaQueueBundle/Resources/public/css/bootstrap.min.css'
		 	'@DkaQueueBundle/Resources/public/css/bootstrap-theme.min.css'
			'@DkaQueueBundle/Resources/public/css/font-awesome.min.css'
		 	'@DkaQueueBundle/Resources/public/css/app.css'
		%}
			<link rel="stylesheet" href="{{ asset_url }}" type="text/css" />
		{% endstylesheets %}
    {% endblock %}
</head>
<body>
    
    <div class="fullBG">
        <span></span>
    </div>
    <div class="wrapper">
        <header>
            <nav class="navbar navbar-default" role="navigation">
			  <div class="container-fluid">
			    <!-- Brand and toggle get grouped for better mobile display -->
			    <div class="navbar-header">
			      <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
			        <span class="sr-only">Toggle navigation</span>
			        <span class="icon-bar"></span>
			        <span class="icon-bar"></span>
			        <span class="icon-bar"></span>
			      </button>
			      <a class="navbar-brand" href="#">DKA Project Q</a>
			    </div>

			    <!-- Collect the nav links, forms, and other content for toggling -->
			    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
			      <ul class="nav navbar-nav">
			        <li><a href="{{ path('users') }}">Users</a></li>
			        <li><a href="{{ path('clients') }}">Clients</a></li>
			        <li><a href="{{ path('queues') }}">Queues</a></li>
			        <li><a href="{{ path('tasks') }}">Tasks</a></li>
			        <li><a href="">Reports</a></li>
			        <li><a href="">Queue</a></li>
			      </ul>
			      <ul class="nav navbar-nav navbar-right">
					<li><a href="{{ path('logout') }}">Log Out</a></li>
			      </ul>
			    </div><!-- /.navbar-collapse -->
			  </div><!-- /.container-fluid -->
			</nav>
        </header>
        <div class="content">
            {% block body %}
                {% block content %}{% endblock %}
            {% endblock %}
        </div>
        <footer>
            <div class="container">
                <div class="row">
					<div class="col-md-12">
                    	<div style="text-align:center;">DKA Design Group &copy; 2014 - All Rights Reserved
					</div>
                </div>
            </div>
        </footer>
    </div>
    
    {% block javascripts %}
        {% javascripts
            '@DkaQueueBundle/Resources/public/js/jquery.js'
            '@DkaQueueBundle/Resources/public/js/bootstrap.min.js'
            '@DkaQueueBundle/Resources/public/js/app.js'
        %}
            <script src="{{ asset_url }}"></script>
        {% endjavascripts %}
    {% endblock %}
</body>
</html>